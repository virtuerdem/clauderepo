
  CREATE OR REPLACE EDITIONABLE PROCEDURE "NORTHI_LOADER"."BACK_TWAMP_WA" AS

XDATA_DATE DATE;

BEGIN
   DBMS_SESSION.SET_NLS('NLS_TERRITORY','''TURKEY''');
   DBMS_SESSION.SET_NLS('NLS_NUMERIC_CHARACTERS','''.,''');
 
    XDATA_DATE:=to_date('20.08.2018','DD.MM.YYYY');
    WHILE XDATA_DATE <=to_date('02.09.2018','DD.MM.YYYY')
    
    LOOP
    
  NORTHI_LOADER.P_TWAMP_PERF_5WA(XDATA_DATE);  
  NORTHI_LOADER.P_TWAMP_PERF_7WA(XDATA_DATE);

  COMMIT;

  XDATA_DATE:=XDATA_DATE+7;

 END LOOP;
  
END;
/
