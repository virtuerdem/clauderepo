
  CREATE OR REPLACE EDITIONABLE PROCEDURE "NORTHI_LOADER"."ALTER_PROC1" 
AS
fieldN VARCHAR2 (100);
XSTATEMENT STRING(1000);
BEGIN
fieldN :='';

FOR v1 IN (SELECT TABLE_NAME FROM NORTHI_LOADER.NORTHI_LOADER_SETTINGS WHERE VENDOR_ID=3

           )
   LOOP
    fieldN := v1.TABLE_NAME;
     LOADER_CREATION.CREATE_LOADER_PROCEDURE(v1.TABLE_NAME);
   --XSTATEMENT := 'EXECUTE LOADER_CREATION.CREATE_LOADER_PROCEDURE(' || fieldN ||') COMMIT;';
     --EXECUTE IMMEDIATE XSTATEMENT ;
   END LOOP;

END;
/
