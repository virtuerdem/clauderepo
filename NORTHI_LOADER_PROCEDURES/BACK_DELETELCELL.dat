
  CREATE OR REPLACE EDITIONABLE PROCEDURE "NORTHI_LOADER"."BACK_DELETELCELL" 
AS
XDATE DATE;

BEGIN
    XDATE:=TO_DATE('19.11.2011 00','DD.MM.YYYY HH24'); 
    WHILE XDATE<=TO_DATE('31.12.2011 23','DD.MM.YYYY HH24')
    
   LOOP
    
            
      DELETE  /*+ PARALLEL 16 */  FROM NORTHI_DATA.LCELL_STATISTICS WHERE FRAGMENT_DATE =  XDATE ;
 
      COMMIT;

     
       
        XDATE:= XDATE+1/24 ;

  END LOOP;
    
   END;
/
