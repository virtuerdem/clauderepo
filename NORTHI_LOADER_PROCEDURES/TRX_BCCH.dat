
  CREATE OR REPLACE EDITIONABLE PROCEDURE "NORTHI_LOADER"."TRX_BCCH" 
AS

BEGIN
DELETE  FROM NORTHI_DATA.TRX_BCCH_LIST;

INSERT INTO NORTHI_DATA.TRX_BCCH_LIST(NE_ID,PARENT_ID,NE_NAME,DATA_DATE,BSC_ID,DESCRIPTION)
   SELECT NE_ID,PARENT_ID,NE_NAME,DATA_DATE,BSC_ID,DESCRIPTION
   FROM NORTHI_PARSER.SR_OBJECTS
   WHERE NE_TYPE=101
   AND DATA_DATE=TRUNC(SYSDATE)-1;
COMMIT;
END; 
/
