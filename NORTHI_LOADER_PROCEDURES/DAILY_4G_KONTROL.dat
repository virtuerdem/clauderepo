
  CREATE OR REPLACE EDITIONABLE PROCEDURE "NORTHI_LOADER"."DAILY_4G_KONTROL" 
AS

BEGIN 

     UPDATE NORTHI_DAILY_PROCESS SET AGGREGATE_STATE=2, PROCESS_COUNT=1 WHERE DATA_DATE=TRUNC(SYSDATE-2) AND SYSTEM_ID = 21; COMMIT;
     
     UPDATE NORTHI_DBH_PROCESS SET BH_STATE=2, PROCESS_COUNT=1 WHERE DATA_DATE=TRUNC(SYSDATE-2) AND SYSTEM_ID = 21; COMMIT;

END;
/
