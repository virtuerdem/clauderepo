
  CREATE OR REPLACE EDITIONABLE PROCEDURE "NORTHI_LOADER"."SQLLDR_TRACKING_ERDEM" AS 
BEGIN 
INSERT INTO    NORTHI_PARSER_sETTINGS.PARSER_SQLLDR_ERDEM  ( 
SELECT TABLE_NAME,DATA_dATE,PARSER_DATE,SYSDATE ADDING_DATE,LOADED_DATA_COUNT,FILE_RECORD_COUNT FROM NORTHI_PARSER_SETTINGS.PARSER_SQLLDR_LOGS WHERE 
DATA_dATE between trunc(sysdate,'hh24')-4/24 and TRUNC(SYSDATE,'hh24')-1/24
 AND TABLE_NAME IN (SELECT PARENT_NAME FROM NORTHI_LOADER.USED_RAW_tABLES WHERE TABLE_NAME='CELLSTS_4G' ) AND OPERATOR_NAME='VODAFONE'  ) ; COMMIT; 

end;
/
