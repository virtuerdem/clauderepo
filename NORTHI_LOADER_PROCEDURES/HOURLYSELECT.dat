
  CREATE OR REPLACE EDITIONABLE PROCEDURE "NORTHI_LOADER"."HOURLYSELECT" (XDATE1 DATE, XDATE2 DATE, VENDORNUM NUMBER)
AS
XN VARCHAR2(400);
BEGIN
SELECT TABLE_NAME INTO XN
FROM  NORTHI_LOADER.NORTHI_LOADER_PROCESS
WHERE LOADER_STATE=0 AND DATA_DATE BETWEEN TO_DATE(XDATE1,'DD.MM.YYYY HH24') AND TO_DATE(XDATE2,'DD.MM.YYYY HH24');
END HOURLYSELECT;

