
  CREATE OR REPLACE EDITIONABLE PROCEDURE "NORTHI_LOADER"."BACK_TWAMP_H" AS

XDATA_DATE DATE;

BEGIN
   DBMS_SESSION.SET_NLS('NLS_TERRITORY','''TURKEY''');
   DBMS_SESSION.SET_NLS('NLS_NUMERIC_CHARACTERS','''.,''');
 
    XDATA_DATE:=to_date('05.01.2020 04','DD.MM.YYYY HH24');
    WHILE XDATA_DATE <=to_date('05.01.2020 04','DD.MM.YYYY HH24')
    
  LOOP
  DELETE FROM NORTHI_DATA.TWAMP_PERF_H WHERE FRAGMENT_DATE = XDATA_DATE;
  COMMIT;
    
  NORTHI_LOADER.P_TWAMP_PERF_H(XDATA_DATE);
  COMMIT;

  XDATA_DATE:=XDATA_DATE+1/24;
 END LOOP;
  
END;
/
